{
    "name": "react-axis",
    "version": "1.0.0",
    "description": "An axis component built with React",
    "keywords": [
        "react",
        "axis",
        "timezones",
        "timezone",
        "time",
        "moment",
        "d3",
        "charts"
    ],
    "homepage": "http://software.es.net/react-axis",
    "author": "ESnet Software Engineering Group <tools@es.net>",
    "repository": "esnet/react-timeseries-charts",
    "main": "dist/entry.js",
    "typings": "dist/entry.d.ts",
    "files": [
        "dist"
    ],
    "bugs": {
        "url": "https://github.com/esnet/react-timeseries-charts/issues"
    },
    "scripts": {
        "docs": "echo \"*** Building API docs\n\" && react-docgen src/components -x js -o ../website/src/api/docs.json --pretty",
        "lint": "eslint src/components/*.js",
        "test": "npm run lint",
        "clean": "rimraf dist",
        "build": "tsc -p tsconfig.json",
        "precommit": "lint-staged",
        "prettier": "prettier --print-width 100 --tab-width 4 --write \"./src/**/*.ts*\""
    },
    "lint-staged": {
        "*.js": [
            "prettier --print-width 100 --tab-width 4 --write",
            "git add"
        ]
    },
    "pre-commit": [
        "lint",
        "build"
    ],
    "license": "BSD-3-Clause-LBNL",
    "dependencies": {
        "@types/d3-format": "^1.3.0",
        "@types/d3-scale": "^2.0.2",
        "@types/moment-timezone": "^0.5.9",
        "@types/react": "^16.7.7",
        "d3-format": "^1.3.2",
        "d3-scale": "^2.1.2",
        "lodash": "^4.17.11",
        "merge": "^1.2.1",
        "moment": "^2.22.2",
        "moment-timezone": "^0.5.23",
        "react-transition-group": "^2.5.0"
    },
    "peerDependencies": {
        "react": "^16.6.3",
        "react-dom": "^16.6.3"
    },
    "devDependencies": {
        "tslib": "^1.9.3",
        "typescript": "3.1.6",
        "react": "16.6.3",
        "react-dom": "16.6.3"
    }
}
