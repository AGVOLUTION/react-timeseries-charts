{
    "name": "react-timeseries-charts",
    "version": "1.0.0",
    "description": "Declarative timeseries charts",
    "keywords": [
        "d3",
        "charts",
        "react",
        "timeseries"
    ],
    "homepage": "http://software.es.net/react-timeseries-charts",
    "author": "ESnet Tools Team <tools@es.net>",
    "repository": "esnet/react-timeseries-charts",
    "main": "dist/entry.js",
    "typings": "dist/entry.d.ts",
    "files": [
        "dist"
    ],
    "bugs": {
        "url": "https://github.com/esnet/react-timeseries-charts/issues"
    },
    "scripts": {
        "docs": "typedoc --json ../website_old/src/api/doc.json --mode modules ./src/*.tsx --ignoreCompilerErrors",
        "lint": "eslint src/components/*.js",
        "test": "npm run lint",
        "clean": "rimraf dist",
        "build": "tsc -p tsconfig.json",
        "rollup": "tsc -p tsconfig.json  && cross-env NODE_ENV=production rollup -c && cross-env NODE_ENV=development rollup -c",
        "start-website": "react-scripts start",
        "build-website": "echo \"*** Building website\n\" && rm -rf docs && react-scripts build && mv build docs",
        "precommit": "lint-staged",
        "prettier": "prettier --print-width 100 --tab-width 4 --write \"./src/**/*.ts*\"",
        "storybook": "start-storybook -p 6006 -c .storybook"
    },
    "lint-staged": {
        "*.js": [
            "prettier --print-width 100 --tab-width 4 --write",
            "git add"
        ]
    },
    "pre-commit": [
        "lint",
        "build"
    ],
    "license": "BSD-3-Clause-LBNL",
    "dependencies": {
        "@types/chroma-js": "^1.4.0",
        "@types/colorbrewer": "^1.0.29",
        "@types/d3-ease": "^1.0.7",
        "@types/d3-format": "^1.3.0",
        "@types/d3-scale": "^2.0.2",
        "@types/d3-shape": "^1.2.6",
        "@types/d3-time-format": "^2.1.0",
        "@types/invariant": "^2.2.29",
        "@types/lodash": "^4.14.118",
        "@types/lodash.isobject": "^3.0.4",
        "@types/moment-duration-format": "^2.2.2",
        "@types/react": "^16.7.7",
        "@types/react-dom": "^16.0.10",
        "array.prototype.fill": "^1.0.2",
        "babel-runtime": "^6.26.0",
        "chroma-js": "^1.4.0",
        "colorbrewer": "^1.3.0",
        "d3-ease": "^1.0.5",
        "d3-format": "^1.3.2",
        "d3-interpolate": "^1.3.2",
        "d3-scale": "^2.1.2",
        "d3-scale-chromatic": "^1.3.3",
        "d3-selection": "^1.3.2",
        "d3-shape": "^1.2.2",
        "d3-time": "^1.0.10",
        "d3-time-format": "^2.1.3",
        "d3-transition": "^1.1.3",
        "flexbox-react": "git+https://github.com/esnet/flexbox-react.git",
        "invariant": "^2.2.4",
        "lodash": "^4.17.11",
        "lodash.isobject": "^3.0.2",
        "merge": "^1.2.1",
        "moment": "^2.22.2",
        "moment-duration-format": "^2.2.2",
        "react-axis": "1.0.0",
        "react-hot-loader": "^4.3.12"
    },
    "devDependencies": {
        "@storybook/addon-info": "^4.0.8",
        "@storybook/addon-knobs": "^4.0.8",
        "@storybook/react": "^4.0.8",
        "@types/jest": "^23.3.9",
        "@types/storybook__addon-info": "^3.4.2",
        "@types/storybook__addon-knobs": "^3.4.1",
        "@types/storybook__react": "^4.0.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^6.26.3",
        "cpx": "^1.5.0",
        "cross-env": "^5.2.0",
        "jest": "^23.6.0",
        "pondjs": "1.0.0-alpha.4",
        "react": "^16.6.3",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "react-dom": "^16.6.3",
        "rimraf": "^2.6.2",
        "storybook-addon-jsx": "^5.4.0",
        "ts-jest": "^23.10.4",
        "typedoc": "^0.13.0",
